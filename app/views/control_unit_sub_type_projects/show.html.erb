<section class="bg-gray-50 dark:bg-gray-900 p-3 sm:p-5">
  <h1 class="mb-4 text-2xl font-bold text-gray-900 dark:text-white">Proyecto: <%= @control_unit_sub_type_project.project.name %></h1>
  <h1 class="mb-4 text-2xl font-bold text-gray-900 dark:text-white">Subtipo: <%= @control_unit_sub_type_project.control_unit_sub_type.name %></h1>

  <div class="py-8 px-4 mx-auto max-w-2xl lg:py-2">

    <%= render @control_unit_sub_type_project %>

    <div class="flex items-center justify-start space-x-4 mt-6">
      <%= link_to "Regresar a subtipo", control_unit_sub_type_path(@control_unit_sub_type_project.control_unit_sub_type), class: "text-primary-700 hover:text-primary-900 dark:text-primary-500 dark:hover:text-primary-700" %>
    </div>
  </div>
  <div class="p-4 rounded-lg bg-gray-50 dark:bg-gray-800">
    <%= turbo_frame_tag 'control_unit_sub_type_projects_extras_turbo_frame_tag', src: control_unit_sub_type_project_extras_path(@control_unit_sub_type_project) %>

  </div>
</section>
